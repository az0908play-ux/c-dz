#include <iostream>
using namespace std;

int main()
{
    const unsigned int N = 6;
    int arr[N];
    srand(time(0));

    for (int i = 0; i < N; i++)
    {
        arr[i] = rand() % 10;
        cout << arr[i] << ' ';
    }
    cout << endl;

    for (int i = 0; i < N - 1; i++) 
    {
        for (int j = 0; j < N - 1 - i; j++) {
            if (arr[j] > arr[j + 1]) {
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }

    for (int i = 0; i < N; i++)
    {
        cout << arr[i] << " ";
    }

    return 0;
}
